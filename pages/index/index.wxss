.index_swiper swiper {
  height: 340rpx;
}
.index_nav {
  display: flex;
}
.index_nav navigator {
  flex: 1;
  padding: 20rpx;
}
.index_floor .floor_group .floor_group_content {
  overflow: hidden;
}
.index_floor .floor_group .floor_group_content navigator {
  float: left;
  width: 33.33%;
  border-right: 5rpx solid #fff;
  border-left: 5rpx solid #fff;
}
.index_floor .floor_group .floor_group_content navigator:nth-last-child(-n+4) {
  height: 207.97413793rpx;
}
.index_floor .floor_group .floor_group_content navigator:nth-last-child(-n+4) image {
  height: 100%;
}
.index_floor .floor_group .floor_group_content navigator:nth-last-child(-n+2) {
  border-top: 5rpx solid #fff;
}
